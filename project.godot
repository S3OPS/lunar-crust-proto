; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Middle-earth Adventure RPG"
config/description="An immersive 3D RPG game set in a Lord of the Rings inspired fantasy world"
run/main_scene="res://scenes/main.tscn"
config/features=PackedStringArray("4.3", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

GameManager="*res://scripts/autoload/game_manager.gd"
EventBus="*res://scripts/autoload/event_bus.gd"
SaveManager="*res://scripts/autoload/save_manager.gd"
QuestManager="*res://scripts/autoload/quest_manager.gd"
InventoryManager="*res://scripts/autoload/inventory_manager.gd"
DialogueManager="*res://scripts/autoload/dialogue_manager.gd"
RegionManager="*res://scripts/autoload/region_manager.gd"
FastTravelManager="*res://scripts/autoload/fast_travel_manager.gd"
FactionManager="*res://scripts/autoload/faction_manager.gd"
CraftingManager="*res://scripts/autoload/crafting_manager.gd"
SpecializationManager="*res://scripts/autoload/specialization_manager.gd"
CompanionManager="*res://scripts/autoload/companion_manager.gd"
SeasonalEventManager="*res://scripts/autoload/seasonal_event_manager.gd"
DifficultyManager="*res://scripts/autoload/difficulty_manager.gd"
AccessibilityManager="*res://scripts/autoload/accessibility_manager.gd"
MultiplayerManager="*res://scripts/autoload/multiplayer_manager.gd"
GuildManager="*res://scripts/autoload/guild_manager.gd"
TradingManager="*res://scripts/autoload/trading_manager.gd"
SocialManager="*res://scripts/autoload/social_manager.gd"
RaidManager="*res://scripts/autoload/raid_manager.gd"
ArenaManager="*res://scripts/autoload/arena_manager.gd"
PrestigeManager="*res://scripts/autoload/prestige_manager.gd"
WorldBossManager="*res://scripts/autoload/world_boss_manager.gd"
MountManager="*res://scripts/autoload/mount_manager.gd"
PetManager="*res://scripts/autoload/pet_manager.gd"
HousingManager="*res://scripts/autoload/housing_manager.gd"
Constants="*res://scripts/utilities/constants.gd"

[debug]

gdscript/warnings/untyped_declaration=1
gdscript/warnings/inferred_declaration=1
gdscript/warnings/unsafe_property_access=1
gdscript/warnings/unsafe_method_access=1
gdscript/warnings/unsafe_cast=1
gdscript/warnings/unsafe_call_argument=1

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/mode=2
window/stretch/mode="canvas_items"

[file_customization]

folder_colors={
"res://scenes/": "blue",
"res://scripts/": "yellow",
"res://scripts/autoload/": "orange",
"res://scripts/components/": "green",
"res://scripts/resources/": "purple"
}

[filesystem]

import/blender/enabled=false
import/fbx/enabled=false

[gui]

common/drop_mouse_on_gui_input_disabled=true

